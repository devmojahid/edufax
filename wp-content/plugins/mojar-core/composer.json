{
    "name": "mojahidul/mojar-core",
    "description": "Mojar Core is a plugin that adds custom post types, custom fields, and custom taxonomies to the WordPress admin.",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Mojahidul Islam",
            "email": "raofahmedmojahid@email.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "htmlburger/carbon-fields-plugin": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "MojarCore\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
